{
  
    "description": "It will display the downloaded item and also can open and remove from it",
    "manifest_version": 2,
    "name": "Download Item",
    "version": "1.0",
    "icons": {
      "20": "icons/page-20.jpg"
    },
  
    "permissions": [
      "downloads",
      "downloads.open"
    ],
  
    "browser_action": {
      "browser_style": true,
      "default_icon": "icons/page-20.jpg",
      "default_title": "Download Item",
      "default_popup": "popup/DownloadItem.html"
    }
  
  }